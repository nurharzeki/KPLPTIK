<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListMatkulDiampuActivity">

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="738dp"
        android:layout_height="702dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="404dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/background_green_circle" />

    <TextView
        android:id="@+id/textListMatkul"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="List Mata Kuliah"
        android:textColor="@color/black"
        android:fontFamily="@font/poppins_semibold"
        android:textStyle="bold"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.122" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvListMatKulDiampu"
        android:layout_width="367dp"
        android:layout_height="544dp"
        tools:listitem="@layout/item_matkul_yang_diampu_dosen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.828"
         />

</androidx.constraintlayout.widget.ConstraintLayout>