<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListKhsDosenActivity"
    android:background="@drawable/background2">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rvListKhsSemesterDosen"
        android:layout_width="370dp"
        android:layout_height="0dp"

        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textviewsekian3" />

    <TextView
        android:id="@+id/ipkDosen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="3.52"
        android:textColor="#003200"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textviewsekian3"
        app:layout_constraintStart_toEndOf="@+id/textviewsekian3"
        app:layout_constraintTop_toTopOf="@+id/textviewsekian3" />

    <TextView
        android:id="@+id/textviewsekian3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:fontFamily="@font/poppins"
        android:text="IPK : "
        android:textColor="#003200"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="@+id/textviewblaaa"
        app:layout_constraintStart_toStartOf="@+id/textviewblaaa"
        app:layout_constraintTop_toBottomOf="@+id/textviewblaaa" />

    <TextView
        android:id="@+id/textviewblaaa"
        android:layout_width="376dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="center"
        android:text="Kartu Hasil Studi"
        android:textColor="#00320A"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textNamaMhsDosen" />

    <TextView
        android:id="@+id/textNamaMhsDosen"
        android:layout_width="376dp"
        android:layout_height="0dp"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/poppins_bold"
        android:gravity="center"
        android:text="Harriko Nur Harzeki"
        android:textColor="#00320A"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>